	<script>
	  
		$('#fileUpload').change(function(){    
			//on change event  
			formdata = new FormData();
			if($(this).prop('files').length > 0)
			{
				file =$(this).prop('files')[0];
				formdata.append("developer", file);
			}
		});

		$(document).ready(function(){
			$("#submitFile").click(function(){              
				jQuery.ajax({
					url: "/upload",
					type: "POST",
					data: formdata,
					processData: false,
					contentType: false,
					success: function (result) {
						// if all is well
						// play the audio file
					}
				});
			});
		});
		
	</script>
