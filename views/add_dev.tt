		<script>
		$(document).ready(function(){
        // click on button submit
		     $("#submitData").on('click', function(){
                 // send ajax
                 $.ajax({
                     url: '/api/v1/admin/dev/add', 
                     type : "POST", 
                     dataType : 'json',
                     data : $("#addDev").serialize(), 
                     success : function(result) {
                         console.log(result);
                     },
                     error: function(xhr, resp, text) {
                         console.log(xhr, resp, text);
                     }
                 })
             });
	    });
		
